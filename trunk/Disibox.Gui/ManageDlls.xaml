<Window x:Class="Disibox.Gui.ManageDlls"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Manage Dlls" MinWidth="370" MinHeight="290" Width="370" Height="290">
    <Grid>
        <ListView Margin="6,6,6,40" Name="listView_Dlls" SelectionMode="Single" />
        <Button Content="Refresh" Margin="0,0,6,6" Name="buttonRefreshDlls" Height="23" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="75" IsDefault="True" TabIndex="1" Click="buttonRefreshDlls_Click" />
        <Button Content="Add" HorizontalAlignment="Left" Margin="6,0,0,6" Name="buttonAddDll" Width="75" Height="23" VerticalAlignment="Bottom" TabIndex="2" Click="buttonAddDll_Click" />
        <Button Content="Delete" HorizontalAlignment="Left" Margin="90,0,0,6" Name="buttonDeleteDll" Width="75" Height="23" VerticalAlignment="Bottom" TabIndex="3" Click="buttonDeleteDll_Click" />
        <Button Content="Download" Margin="175,0,0,6" Name="buttonDownloadDll" Height="23" VerticalAlignment="Bottom" TabIndex="4" Click="buttonDownloadDll_Click" HorizontalAlignment="Left" Width="75" />
    </Grid>
</Window>
